{
  "name": "arbundles",
  "version": "0.6.17-rc1",
  "description": "Arweave bundling library",
  "author": "Josh Benaron <joshbenaron@gmail.com>",
  "license": "Apache-2.0",
  "main": "src/index.js",
  "browser": "esm/shim.js",
  "types": "index.d.ts",
  "files": [
    "index.js",
    "index.js.map",
    "index.d.ts",
    "src/**/*.js",
    "src/**/*.js.map",
    "src/**/*.d.ts",
    "stream/**/*.js",
    "stream/**/*.js.map",
    "stream/**/*.d.ts",
    "file/**/*.js",
    "file/**/*.js.map",
    "file/**/*.d.ts",
    "esm/**/*.js",
    "esm/**/*.js.map",
    "esm/**/*.d.ts"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json && tsc -p esm.tsconfig.json",
    "lint": "eslint ./src -c ./.eslintrc.json --ext .ts,.tsx",
    "test": "jest --coverage",
    "husky-install": "(node -e \"if (!require('fs').existsSync(__dirname + '/.git')) {process.exit(1)}\" && husky install || true)",
    "ensure-build": "(node -e \"if (!require('fs').existsSync(__dirname + '/build')) {process.exit(1)}\" || yarn run build)",
    "prepack": "yarn run ensure-build",
    "prepare": "yarn run ensure-build"
  },
  "devDependencies": {
    "@noble/ed25519": "^1.6.0",
    "@randlabs/myalgo-connect": "^1.1.2",
    "@solana/wallet-adapter-base": "^0.9.2",
    "@types/bs58": "^4.0.1",
    "@types/jest": "~26.0.23",
    "@types/multistream": "^2.1.1",
    "@types/node": "^17.0.10",
    "@types/secp256k1": "^4.0.3",
    "@typescript-eslint/eslint-plugin": "~4.25.0",
    "@typescript-eslint/parser": "~4.25.0",
    "algosdk": "^1.13.1",
    "arweave": "^1.10.18",
    "axios": "^0.21.3",
    "base64url": "^3.0.1",
    "bs58": "^4.0.1",
    "eslint": "~7.27.0",
    "eslint-config-prettier": "~8.3.0",
    "eslint-plugin-jest": "~24.3.6",
    "ethers": "^5.5.1",
    "husky": "^7.0.0",
    "jest": "~27.0.3",
    "keccak": "^3.0.2",
    "object-sizeof": "^1.6.1",
    "perf_hooks": "^0.0.1",
    "prettier": "~2.3.0",
    "pretty-quick": "^3.1.2",
    "rimraf": "~3.0.2",
    "ts-jest": "~27.0.2",
    "ts-loader": "^9.2.8",
    "ts-node": "^10.5.0",
    "tslib": "^2.3.0",
    "tsutils": "~3.21.0",
    "typescript": "~4.3.2",
    "webpack": "^5.70.0"
  },
  "dependencies": {
    "arweave": "^1.10.18",
    "arweave-stream-tx": "^1.1.0",
    "avsc": "https://github.com/Bundlr-Network/avsc#csp-fixes",
    "axios": "^0.21.3",
    "base64url": "^3.0.1",
    "bs58": "^4.0.1",
    "crypto-browserify": "^3.12.0",
    "multistream": "^4.1.0",
    "path-browserify": "^1.0.1",
    "secp256k1": "^4.0.2",
    "stream-browserify": "^3.0.0",
    "tmp-promise": "^3.0.2"
  },
  "optionalDependencies": {
    "@noble/ed25519": "^1.6.0",
    "@randlabs/myalgo-connect": "^1.1.2",
    "@solana/wallet-adapter-base": "^0.9.2",
    "algosdk": "^1.13.1",
    "arweave": "^1.10.18",
    "axios": "^0.21.3",
    "base64url": "^3.0.1",
    "bs58": "^4.0.1",
    "buffer": "npm:buffer",
    "ethers": "^5.5.1",
    "keccak": "^3.0.2",
    "ts-node": "^10.5.0",
    "tslib": "^2.3.0"
  }
}
